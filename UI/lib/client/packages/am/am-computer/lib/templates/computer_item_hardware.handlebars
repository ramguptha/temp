    </tr>
    <tr>
      <td>{{t 'amData.computerHardwareTabSpec.swapSpaceFree'}}</td>
      <td>{{relatedHardwareController.model.presentation.swapSpaceFree}}</td>
    </tr>
    {{#if isMacPlatform}}
      <tr>
        <td>{{t 'amData.computerHardwareTabSpec.swapSpaceEncrypted'}}</td>
        <td>{{relatedHardwareController.model.presentation.swapSpaceEncrypted}}</td>
      </tr>
    {{/if}}
    <tr>
