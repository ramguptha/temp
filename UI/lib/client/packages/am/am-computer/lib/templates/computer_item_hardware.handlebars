<div class="snap-content-container">
  <div class="all-details-content">

  <div class="snap-content-title"><h3>{{t 'amComputer.hardwareTab.title'}}</h3>
  </div>

  <div class="snap-content-icon"><img src={{deviceIconPath}}>
  </div>

  <div class="device-main-description">
    <h4 class="device-model">{{machineModel}}</h4>
    <div class="device-details-table-heading">
      <div class="table-heading-row">
        <div class="device-details-table-cell">{{t 'amData.computerHardwareTabSpec.computerSerialNumber'}}<p>{{relatedHardwareController.model.presentation.computerSerialNumber}}</p></div>
        <div class="device-details-table-cell">{{t 'amData.computerHardwareTabSpec.primaryMacAddress'}}<p>{{relatedHardwareController.model.presentation.primaryMacAddress}}</p></div>
      </div>
    </div>
  </div>

  <br/>
  <table class="device-details-table-body">
    <tbody>
    <tr>
      <td>{{t 'amData.computerHardwareTabSpec.dateTime'}}</td>
      <td>{{relatedHardwareController.model.presentation.dateTime}}</td>
    </tr>
    <tr>
      <td>{{t 'amData.computerHardwareTabSpec.bootRomInformation'}}</td>
      <td>{{relatedHardwareController.model.presentation.bootRomInformation}}</td>
    </tr>
    <tr>
