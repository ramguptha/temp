    </table>
  </div>

  <div class="device-actions">
    <div class="device-actions-title"><span class="icon-checkmark1"></span>

      <h3>{{t 'amComputer.aboutComputerTab.commandsTitle'}}</h3></div>
    <div class="device-actions-list">
      <ul class="actions-list">
        <li>
          <a href="#" class="is-button-for-send-message"{{action "sendMessageCommand"}}>
            <span class="icon-action"></span>
            <span class="record-action-name">{{t 'amComputer.computerListPage.commands.sendMessage'}}</span>

            <p
              class="record-action-description">{{t 'amComputer.computerListPage.commands.sendMessageDescription' agentName=model.presentation.agentName}}</p>
          </a>
        </li>
        <li>
          <a href="#" class="is-button-for-gather-inventory"{{action "gatherInventoryCommand"}}>
            <span class="icon-action"></span>
            <span class="record-action-name">{{t 'amComputer.computerListPage.commands.gatherInventory'}}</span>

            <p
              class="record-action-description">{{t 'amComputer.computerListPage.commands.gatherInventoryDescription' agentName=model.presentation.agentName}}</p>
          </a>
        </li>
        {{#if isFreezeCommandSupported}}
          <li>
            <a href="#" class="is-button-for-device-freeze"{{action "deviceFreezeCommand"}}>
              <span class="icon-action"></span>
              <span class="record-action-name">{{t 'amComputer.computerListPage.commands.deviceFreeze'}}</span>

              <p
                class="record-action-description">{{t 'amComputer.computerListPage.commands.deviceFreezeDescription' agentName=model.presentation.agentName}}</p>
            </a>
          </li>
        {{/if}}
        {{#if isUnfreezeCommandSupported}}
          <li>
            <a href="#" class="is-button-for-device-unfreeze"{{action "deviceUnfreezeCommand"}}>
              <span class="icon-action"></span>
              <span class="record-action-name">{{t 'amComputer.computerListPage.commands.deviceUnfreeze'}}</span>

              <p
                class="record-action-description">{{t 'amComputer.computerListPage.commands.deviceUnfreezeDescription' agentName=model.presentation.agentName}}</p>
            </a>
          </li>
        {{/if}}
        {{#if isDataDeleteCommandSupported}}
          <li>
            <a href="#" class="is-button-for-data-delete" {{action "dataDeleteCommand"}}>
              <span class="icon-action"></span>
              <span class="record-action-name">{{t 'amComputer.computerListPage.commands.dataDelete'}}</span>

              <p
                class="record-action-description">{{t 'amComputer.computerListPage.commands.dataDeleteDescription' agentName=model.presentation.agentName}}</p>
            </a>
          </li>
        {{/if}}
      </ul>
    </div>

  </div>

</div>
