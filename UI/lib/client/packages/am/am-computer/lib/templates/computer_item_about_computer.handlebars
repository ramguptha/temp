<div class="snap-content-container">

  <div class="all-details-content">

    <div class="snap-content-title"><h3>{{t 'amComputer.aboutComputerTab.title'}}</h3>
    </div>

    <div class="snap-content-icon"><img src={{deviceIconPath}}></div>
    <div class="device-main-description">
      <h4 class="device-model">{{machineModel}}</h4>

      <div class="device-details-table-heading">
        <div class="table-heading-row">
          <div class="device-details-table-cell"><span class="os-version"><img
            src={{osIconPath}}></span>{{t 'amData.computerItemAgentInformationSpec.osPlatform'}}
            <p>{{model.presentation.osPlatform}}</p></div>
          <div class="device-details-table-cell">{{t 'amData.computerItemAgentInformationSpec.osVersion'}}
            <p>{{model.presentation.osVersion}}</p></div>
          <div class="device-details-table-cell">{{t 'amData.computerItemAgentInformationSpec.lastHeartbeat'}}
            <p>{{model.presentation.lastHeartbeat}}</p></div>
          <div class="device-details-table-cell">{{t 'amData.computerItemAgentInformationSpec.recordCreationDate'}}
            <p>{{model.presentation.recordCreationDate}}</p></div>
        </div>
      </div>
    </div>

    <br>
    <table class="device-details-table-body">
