      </tr>
      <tr>
        <td>{{t 'amData.computerItemAgentInformationSpec.useOnlyAbsoluteManageForOsUpdates'}}</td>
        <td>{{model.presentation.useOnlyAbsoluteManageForOsUpdates}}</td>
      </tr>
      <tr>
        <td>{{t 'amData.computerItemAgentInformationSpec.absoluteRemoteEnabled'}}</td>
        <td>{{model.presentation.absoluteRemoteEnabled}}</td>
      </tr>
      <tr>
        <td>{{t 'amData.computerItemAgentInformationSpec.absoluteRemotePort'}}</td>
        <td>{{model.presentation.absoluteRemotePort}}</td>
