<div class="snap-content-container">
  <div class="all-details-content">
    <div class="snap-content-title"><h3>{{t 'amMobileDevice.devicePage.aboutDeviceTab.title'}}</h3>
    </div>
    <div class="snap-content-icon"><img src={{deviceIconPath}}></div>

    <div class="device-main-description">
      <h4 class="device-model">{{model.presentation.model}}</h4>

      <p>{{t 'amMobileDevice.devicePage.aboutDeviceTab.phoneNumber' phoneNumber=model.presentation.phoneNumber}}</p>

      <div class="device-details-table-heading">
        <div class="table-heading-row">
          <div class="device-details-table-cell"><span class="os-version"><img
            src={{osIconPath}}></span>{{t 'amMobileDevice.devicePage.aboutDeviceTab.osVersion'}}
            <p>{{model.presentation.osPlatform}} {{model.presentation.osVersion}}</p></div>
          <div class="device-details-table-cell">{{t 'amMobileDevice.devicePage.aboutDeviceTab.lastContact'}}
            <p>{{model.presentation.lastContact}}</p></div>
        </div>
        {{#if isNotWinPhoneDevice}}
          <div class="table-heading-row">
            <div class="device-details-table-cell"><span
              class={{passcodePresentIconClass}}></span>{{t 'amMobileDevice.devicePage.aboutDeviceTab.passcodePresent'}}
              <p>{{model.presentation.isPasscodePresent}}</p></div>
            <div class="device-details-table-cell">
              <span class={{batteryLevelIconClass}}></span>
              <span class="device-underscore-text">{{t 'amMobileDevice.devicePage.aboutDeviceTab.batteryLevel'}}
                <p>{{model.presentation.batteryLevel}}%</p></span>
            </div>
          </div>
        {{/if}}
      </div>
    </div>

