<div class="modal-action-icon"><span class="icon-square-attention1"></span></div>
<div class="modal-action-content">
<div class="modal-action-confirmation">
    <h3>{{t 'shared.timeoutWarning.header'}}</h3>
    <p>{{t 'shared.timeoutWarning.description' numberOfSeconds=secsToExpiry}}</p>
</div>
<div class="container-fluid modal-action-buttons">
    <div class="button-group" >
        <button class="btn btn-action" {{action "onOK"}}>{{t 'shared.buttons.ok'}}</button>
    </div>
</div>
</div>